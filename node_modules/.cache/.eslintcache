ningCount":0},{"filePath":"92","messages":"93","suppressedMessages":"94","errorCount":0,"fatalErrorCount":0,"warningCount":2,"fixableErrorCount":0,"fixableWarningCount":0,"source":null},{"filePath":"95","messages":"96","suppressedMessages":"97","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/farhanahossain/Desktop/SEI/projects/PlantItForward/pif-ui/src/index.js",[],[],[],"/Users/farhanahossain/Desktop/SEI/projects/PlantItForward/pif-ui/src/reportWebVitals.js",[],[],"/Users/farhanahossain/Desktop/SEI/projects/PlantItForward/pif-ui/src/App.js",["98"],[],"import { useState, useEffect } from \"react\";\nimport { Routes, Route } from \"react-router-dom\";\nimport LandingHome from \"./pages/LandingHome\";\nimport NewItem from \"./pages/NewItem\";\nimport Layout from \"./components/Layout/Layout\";\nimport ItemEdit from \"./pages/ItemEdit\";\nimport ItemView from \"./pages/ItemView\";\nimport userService from \"./utils/userService\";\nimport LoginTest from \"./pages/Login\";\nimport SignupTest from \"./pages/Signup\";\nimport bootstrap from \"bootstrap\";\nimport About from \"./pages/About\";\n\n\nfunction App() {\n\n  const [items, setItems] = useState([]);\n  const [user, setUser] = useState({});\n\n\n  useEffect(() => {\n    fetch(\"http://localhost:8000/item/\")\n      .then(res => res.json())\n      .then(items => setItems(items));\n  }, []);\n\n  const handleSignupOrLogin = () => {\n      setUser(userService.getUser())\n  }\n\n  const handleLogout = () => {\n      userService.logout();\n      setUser(null)\n  }\n\n  const addItem = (item) => {\n    setItems([...items, item]);\n  };\n  \n\n  const updateItemState = (id) => {\n    setItems(items.filter((item) => item._id !== id));\n  };\n\n  \n\n  return (\n    <Layout user={user} setUser={setUser} handleLogout={handleLogout}>\n      <Routes>\n       <Route\n          path=\"/\"\n          element={\n            <LandingHome items={items} updateItemState={updateItemState} user={user} />\n          }\n        />\n        <Route path=\"/new-item\" element={<NewItem addItem={addItem} />} />\n        <Route\n          path=\"/item/edit/:id/\"\n          element={<ItemEdit setItems={setItems} />}\n        />\n        <Route path=\"/item/:id\" element={<ItemView items={items} />} />\n        <Route path=\"/login\" element={<LoginTest handleSignupOrLogin={handleSignupOrLogin} setUser={setUser} />} />\n        <Route path=\"/signup\" element={<SignupTest handleSignupOrLogin={handleSignupOrLogin}/>}/>\n      <Route path=\"/about\" element={<About />} />\n      </Routes>\n    </Layout>\n  );\n}\n\nexport default App;\n","/Users/farhanahossain/Desktop/SEI/projects/PlantItForward/pif-ui/src/pages/NewItem.jsx",[],[],"/Users/farhanahossain/Desktop/SEI/projects/PlantItForward/pif-ui/src/pages/Login.jsx",[],[],"/Users/farhanahossain/Desktop/SEI/projects/PlantItForward/pif-ui/src/pages/ItemView.jsx",["99"],[],"import React, {useState, useEffect} from 'react'\nimport { Link, useParams } from 'react-router-dom'\n\nconst ItemView = ({items}) => {\n\n    let {id} = useParams()\n\n    const [item, setItem] = useState()\n    \n    useEffect(() => {\n        fetch(`http://localhost:8000/item/${id}`)\n        .then(res => res.json())\n        .then(data => setItem(data))\n    },[])\n\n    //console.log(item)\n\n  return (\n    <div>\n        {\n        item && (<>\n        <h1>{item.name}</h1>\n        <img src={item.images} alt={item.description} />\n        <p>{item.description}</p>\n\n        <Link to={`/item/edit/${item._id}`} > Edit Item </Link>\n        </>)\n        }\n    </div>\n  )\n}\n\nexport default ItemView","/Users/farhanahossain/Desktop/SEI/projects/PlantItForward/pif-ui/src/pages/ItemEdit.jsx",[],[],"/Users/farhanahossain/Desktop/SEI/projects/PlantItForward/pif-ui/src/components/Layout/Layout.jsx",[],[],"/Users/farhanahossain/Desktop/SEI/projects/PlantItForward/pif-ui/src/components/Item/Items.jsx",[],[],"/Users/farhanahossain/Desktop/SEI/projects/PlantItForward/pif-ui/src/components/Navbar/Navbar.jsx",["100"],[],"import React from \"react\";\nimport styled from \"styled-components\";\nimport { Link, useNavigate } from \"react-router-dom\";\n\nconst NavbarContainer = styled.nav`\n  background-color: darkgreen;\n  padding: 5px;\n\n  ul {\n    display: flex;\n    justify-content: space-evenly;\n  }\n  li {\n    list-style: none;\n  }\n`;\n\nconst Navbar = ({ user, handleLogout }) => {\n  //console.log(user)\n  let nav = user ? \n      <ul>\n        <li>\n          {\" \"}\n          <Link to=\"/\"> Home </Link>\n        </li>\n        <li>\n          {\" \"}\n          <Link to=\"/new-item\"> Trade </Link>\n        </li>\n        <li>\n          {\" \"}\n          <Link to=\"/items\"> Previous Orders </Link>\n        </li>\n        <li>\n          {\" \"}\n          <Link to=\"/items/:id\"> Order </Link>\n        </li>\n        <li>\n          {\" \"}\n          <Link to=\"/about\"> About </Link>\n        </li>\n        <li>\n          {\" \"}\n          Hello @{user.name}\n        </li>\n        <li>\n          <Link to='' onClick={handleLogout}>Log Out</Link>\n        </li>\n      </ul> \n      :\n       <ul>\n         <li>\n          {\" \"}\n          <Link to=\"/\"> Home </Link>\n        </li>\n        <li>\n          {\" \"}\n          <Link to=\"/login\"> Login </Link>\n        </li><li>\n          {\" \"}\n          <Link to=\"/signup\"> Signup </Link>\n        </li>\n       </ul>\n\n    return (\n      <NavbarContainer>\n        {nav}\n      </NavbarContainer>\n    )\n  \n};\n\nexport default Navbar;\n","/Users/farhanahossain/Desktop/SEI/projects/PlantItForward/pif-ui/src/components/Item/Item.jsx",[],[],"/Users/farhanahossain/Desktop/SEI/projects/PlantItForward/pif-ui/src/utils/tokenService.jsx",[],[],"/Users/farhanahossain/Desktop/SEI/projects/PlantItForward/pif-ui/src/pages/LandingHome.jsx",["101"],[],"import Items from '../components/Item/Items'\nimport {Link} from 'react-router-dom'\nimport Logo4 from '../assets/images/Logo4.png'\n\nconst Home = ({ items, updateItemState, user }) => {\n if(user){\n  return (\n    <div>\n      <Items items={items} updateItemState={updateItemState} user={user} />\n    </div>\n  )\n}\n  else {\n    return (\n      <div >\n        <img src={Logo4}  alt=''/>\n      </div>\n    )\n  }\n\n};\n\nexport default Home;\n","/Users/farhanahossain/Desktop/SEI/projects/PlantItForward/pif-ui/src/utils/userService.jsx",[],[],"/Users/farhanahossain/Desktop/SEI/projects/PlantItForward/pif-ui/src/pages/Signup.jsx",["102"],[],"import axios from 'axios';\nimport React, {useState} from 'react';\nimport { useNavigate } from 'react-router-dom';\nimport userService from '../utils/userService'\n\nconst SignupTest = ({handleSignupOrLogin}) => {\n\n    const navigate = useNavigate()\n    const [formData, setFormData] = useState({\n        name: '', \n        password: '', \n        passwordConf: ''\n    })\n\n    const handleChange = (e) => {\n        setFormData({...formData, [e.target.id] : e.target.value})\n    }\n\n    const handleSubmit = async (e) => {\n        e.preventDefault();\n        try{\n          await userService.signup(formData);\n          handleSignupOrLogin();\n          navigate('/')\n        }\n        catch(err){\n          console.error('signup did not work')\n        }\n    }\n\n  return (\n    <div onSubmit={handleSubmit} className=\"min-h-full flex items-center justify-center py-12 px-4 sm:px-6 lg:px-8\">\n        <title>Sign Up</title>\n    <div className=\"max-w-md w-full space-y-8\">\n      <form className=\"mt-8 space-y-6\">\n        <input type=\"hidden\" name=\"remember\" defaultValue=\"true\" />\n        <div className=\"rounded shadow-sm\">\n          <div>\n            <label htmlFor=\"name\" className=\"sr-only\">\n            </label>\n            <input\n              id=\"name\"\n              name=\"name\"\n              type=\"text\"\n              required\n              className=\"appearance-none rounded-none relative block\n              w-full px-3 py-2 border border-gray-300\n              placeholder-gray-500 text-gray-900 rounded-t-md\n              focus:outline-none focus:ring-green-700\n              focus:border-green-700 focus:z-10 sm:text-sm\"\n              placeholder=\"Username\"\n              onChange={handleChange}\n            />\n          </div>\n          <div>\n            <label htmlFor=\"password\" className=\"sr-only\">\n            </label>\n            <input\n              id=\"password\"\n              name=\"password\"\n              type=\"password\"\n              required\n              className=\"appearance-none rounded-none relative block\n              w-full px-3 py-2 border border-gray-300\n              placeholder-gray-500 text-gray-900 rounded-b-md\n              focus:outline-none focus:ring-green-700\n              focus:border-green-700 focus:z-10 sm:text-sm\"\n              placeholder=\"Password\"\n              onChange={handleChange}\n            />\n          </div>\n          <div>\n            <label htmlFor=\"passwordConf\" className=\"sr-only\">\n            </label>\n            <input\n              id=\"passwordConf\"\n              name=\"passwordConf\"\n              type=\"password\"\n              required\n              className=\"appearance-none rounded-none relative block\n              w-full px-3 py-2 border border-gray-300\n              placeholder-gray-500 text-gray-900 rounded-b-md\n              focus:outline-none focus:ring-green-700\n              focus:border-green-700 focus:z-10 sm:text-sm\"\n              placeholder=\"Confirm Password\"\n              onChange={handleChange}\n            />\n          </div>\n        </div>\n        <div>\n          <button\n            type=\"submit\"\n            className=\"group relative w-full flex justify-center\n            py-2 px-4 border border-transparent text-sm font-medium\n            rounded-md text-white bg-green-700 hover:bg-green-900\n            focus:outline-none focus:ring-2 focus:ring-offset-2\n            focus:ring-green-500\"\n          >\n            Sign Up\n          </button>\n        </div>\n      </form>\n    </div>\n  </div>\n  )\n}\n\nexport default SignupTest","/Users/farhanahossain/Desktop/SEI/projects/PlantItForward/pif-ui/src/pages/About.jsx",[],[],"/Users/farhanahossain/Desktop/SEI/projects/PlantItForward/pif-ui/src/components/Map/Maps.jsx",["103","104"],[],"/Users/farhanahossain/Desktop/SEI/projects/PlantItForward/pif-ui/src/components/Map/geolocation.jsx",[],[],{"ruleId":"105","severity":1,"message":"106","line":11,"column":8,"nodeType":"107","messageId":"108","endLine":11,"endColumn":17},{"ruleId":"109","severity":1,"message":"110","line":14,"column":7,"nodeType":"111","endLine":14,"endColumn":9,"suggestions":"112"},{"ruleId":"105","severity":1,"message":"113","line":3,"column":16,"nodeType":"107","messageId":"108","endLine":3,"endColumn":27},{"ruleId":"105","severity":1,"message":"114","line":2,"column":9,"nodeType":"107","messageId":"108","endLine":2,"endColumn":13},{"ruleId":"105","severity":1,"message":"115","line":1,"column":8,"nodeType":"107","messageId":"108","endLine":1,"endColumn":13},{"ruleId":"105","severity":1,"message":"116","line":2,"column":29,"nodeType":"107","messageId":"108","endLine":2,"endColumn":44},{"ruleId":"109","severity":1,"message":"117","line":21,"column":6,"nodeType":"111","endLine":21,"endColumn":8,"suggestions":"118"},"no-unused-vars","'bootstrap' is defined but never used.","Identifier","unusedVar","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'id'. Either include it or remove the dependency array.","ArrayExpression",["119"],"'useNavigate' is defined but never used.","'Link' is defined but never used.","'axios' is defined but never used.","'MarkerClusterer' is defined but never used.","React Hook useEffect has missing dependencies: 'lat' and 'lng'. Either include them or remove the dependency array. 